package analysis;

import static org.junit.Assert.*;

import org.junit.Test;

public class SearchInSortedRotatedArrayTest {

	@Test
	public void test() {
		SearchInSortedRotatedArray search = new SearchInSortedRotatedArray();
		int inpArr[] = { 15, 16, 19, 20, 25, 1, 3, 4, 5, 7, 10, 14 };
		assertEquals(1, search.searchInRotatedArray(inpArr, 16));
	}
}
